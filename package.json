{
  "name": "ngx-color-selector",
  "version": "0.1.8",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ngx-color-selector -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alisalama/ngx-color-selector"
  },
  "author": {
    "name": "Jeremy Ramsey",
    "email": "jeremy@bytelabs.co"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alisalama/ngx-color-selector/issues"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "2.5.53",
    "@types/node": "~6.0.60",
    "concurrently": "^3.4.0",
    "core-js": "^2.6.12",
    "del": "^2.2.2",
    "gulp": "^4.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.17.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.6.1",
    "node-sass": "^5.0.0",
    "node-sass-tilde-importer": "^1.0.0",
    "node-watch": "^0.5.9",
    "protractor": "~7.0.0",
    "rollup": "^0.49.3",
    "run-sequence": "^1.2.2",
    "rxjs": "^6.6.3",
    "systemjs": "^0.20.12",
    "ts-node": "~3.2.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "zone.js": "~0.10.3"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "@angular/cli": "^11.0.2",
    "rxjs-compat": "^6.0.0-rc.0",
    "tslib": "^1.14.1"
  }
}
